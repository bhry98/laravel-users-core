{
  "name": "bhry98/laravel-users-core",
  "description": "laravel packages for manage users and auth with jwt",
  "type": "library",
  "require": {
    "laravel/framework": "^11.31",
    "tymon/jwt-auth": "^2.0"
  },
  "license": "MIT",
  "version": "0.0.1",
  "autoload": {
    "psr-4": {
      "Bhry98\\LaravelUsersCore\\": "src/"
    }
  },
  "authors": [
    {
      "name": "bhry98"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "@php artisan vendor:publish --tag=bhry98-users-core --force",
      "@php artisan vendor:publish --tag=bhry98-users-core --force"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bhry98\\LaravelUsersCore\\LaravelUsersCoreServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
